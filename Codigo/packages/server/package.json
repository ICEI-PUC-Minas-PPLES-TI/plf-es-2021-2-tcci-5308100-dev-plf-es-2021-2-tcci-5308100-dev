{
  "name": "@sec/server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "ISC",
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:dev": "NODE_ENV=development nest build",
    "build:prod": "NODE_ENV=production nest build",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "dev": "yarn start:dev",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "NODE_ENV=MIGRATION ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli -f ./ormconfig-migrations.ts",
    "orm:generate": "npm run typeorm:cli -- migration:generate -n database-migration",
    "orm:run": "npm run typeorm:cli -- migration:run",
    "orm:revert": "npm run typeorm:cli -- migration:revert",
    "orm:seed": "NODE_ENV=SEEDING ts-node -r tsconfig-paths/register --"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.58.0",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.0.3",
    "@sec/common": "1.0.0",
    "@shopify/shopify-api": "^2.1.0",
    "aws-sdk": "^2.1104.0",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "handlebars": "^4.7.7",
    "helmet": "^5.0.2",
    "moment": "^2.29.1",
    "nest-winston": "^1.6.2",
    "nodemailer": "^6.7.3",
    "objects-to-csv": "^1.3.6",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.45",
    "winston": "^3.5.1",
    "winston-daily-rotate-file": "^4.6.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
